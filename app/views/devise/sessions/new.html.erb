<div class="login">
    <br />
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="field">
        <%= f.email_field :email, autofocus: true, placeholder: "Email",class: "inputbis" %>
    </div>

    <div class="field">
        <%= f.password_field :password, autocomplete: "off", placeholder: "Mot de Passe",class: "inputbis" %>
    </div>

    <div class="actions" >
        <%= f.submit "Se Connecter",class: "btn btn-primary btn-block btn-large" %>
    </div>
    <% end %>
    <br />
    <%= render "devise/shared/links" %>
</div>
